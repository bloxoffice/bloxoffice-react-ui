{
  "presets": [
    "es2015",
    "react",
    "stage-0"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "components": "./src/components",
          "constants": "./src/constants",
          "containers": "./src/containers",
          "utils": "./src/utils",
          "images": "./src/assets/images"
        }
      }
    ]
  ]
}
